<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\MiniChess\MiniChess.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="ChessPieces.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>ChessPieces.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="ChessPieces.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>ChessPieces.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>